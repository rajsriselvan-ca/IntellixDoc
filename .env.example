# LLM Provider: groq | ollama | openai | claude
LLM_PROVIDER=groq

# API Keys (get Groq free key at https://console.groq.com)
GROQ_API_KEY=
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Optional: Ollama (local, no key needed)
OLLAMA_BASE_URL=http://localhost:11434

# Embedding model (default works for most cases)
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2

# Backend/Worker use these when running with Docker Compose
# REDIS_URL, QDRANT_URL, DATABASE_URL are set in docker-compose for services
